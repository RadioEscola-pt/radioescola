{
    "configurations": [
    
        {
            "name": "Build Docker build",
            "type": "debugpy",
            "request": "launch",
            "program": "docker.py",
            "console": "integratedTerminal",
            "args": "build",
        },
        {
            "name": "Run Docker Image",
            "type": "debugpy",
            "request": "launch",
            "program": "docker.py",
            "console": "integratedTerminal",
            "args": "launch",
        },
        {
            "name": "Run Docker Db Image",
            "type": "debugpy",
            "request": "launch",
            "program": "docker.py",
            "console": "integratedTerminal",
            "args": "db",

        },
        {
            "name": "Update Docker Image",
            "type": "debugpy",
            "request": "launch",
            "program": "docker.py",
            "console": "integratedTerminal",
            "args": "refresh",
        },
        {
            "name": "Update Docker no Database Image",
            "type": "debugpy",
            "request": "launch",
            "program": "docker.py",
            "console": "integratedTerminal",
            "args": "noDb",
        },
        


        {
            "type": "firefox",
            "request": "launch",
            "reAttach": true,
            "name": "Launch localhost",
            "url": "http://localhost:3000",
            "webRoot": "${workspaceFolder}"
        }
    ]
}