{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Docker: Build",
			"type": "shell",
			"command": "python3",
			"args": [
				"docker.py",
				"build"
			],
			"options": {
				"cwd": "${workspaceFolder}/"
			  }
		},
		{
			"label": "Docker: Refresh",
			"command": "python3",
			"args": [
				"docker.py",
				"refresh"
			],
			"options": {
				"cwd": "${workspaceFolder}/"
			  }
		},
		{
			"label": "Docker: Launch",
			"type": "shell",
			"command": "python3",
			"args": [
				"docker.py",
				"launch"
			],
			"options": {
				"cwd": "${workspaceFolder}/"
			  }
		},
		{
			"label": "Docker: Db Launch",

			"command": "python",
			"args": [
				"docker.py",

				"db"
			],
			"options": {
				"cwd": "${workspaceFolder}/"
			  }

		}
	]
}